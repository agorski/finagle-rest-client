<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <target>System.out</target>
        <encoder>
            <pattern>%date{YYYY.MM.dd HH:mm:ss} [%logger{0}]: %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="albgorski" level="INFO" />

    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>
